<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mycompany.csgonades.Cache"
    android:padding="16dp"
    android:background="@drawable/nadesback">

        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/scrollView"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:fillViewport="false"
            android:layout_alignParentEnd="false"
            android:layout_alignParentRight="false"
            android:layout_marginBottom="60dp">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Smokes"
                    android:id="@+id/textView"
                    android:textStyle="bold"
                    android:textSize="30dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true" />
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="9"
                    android:orientation="horizontal"
                    android:layout_below="@id/textView">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:id="@+id/tside"
                    android:orientation="vertical"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="T Side"
                        android:id="@+id/textView2"

                        android:layout_gravity="center_horizontal"

                        android:layout_below="@+id/textView"
                        android:layout_toLeftOf="@+id/textView"
                        android:layout_toStartOf="@+id/textView" />
                    <Button
                        android:layout_width="fill_parent"
                        android:layout_height="75dp"
                        android:text="from B Toxic to tree room"
                        android:id="@+id/tsmoke5"

                        android:onClick="smoke1"

                        android:layout_below="@+id/textView3"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />
                    <Button
                        android:layout_width="fill_parent"
                        android:layout_height="75dp"
                        android:text="from garage to left side of mid"
                        android:id="@+id/tsmoke4"

                        android:onClick="smoke3"

                        android:layout_below="@+id/tsmoke5"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />
                    <Button
                        android:layout_width="fill_parent"
                        android:layout_height="75dp"
                        android:text="from garage to right side of mid"
                        android:id="@+id/tsmoke3"

                        android:onClick="smoke5"

                        android:layout_below="@+id/tsmoke4"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />
                    <Button
                        android:layout_width="fill_parent"
                        android:layout_height="75dp"
                        android:text="from b toxic to b generator"
                        android:id="@+id/tsmoke2"

                        android:onClick="smoke7"

                        android:layout_below="@+id/tsmoke3"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />
                    <Button
                        android:layout_width="fill_parent"
                        android:layout_height="75dp"
                        android:text="from a main to a site"
                        android:id="@+id/tsmoke1"


                        android:onClick="smoke9"
                        android:layout_below="@+id/tsmoke2"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />
                    <Button
                        android:layout_width="fill_parent"
                        android:layout_height="75dp"
                        android:text="from t spawn to ct connector"
                        android:id="@+id/tsmoke6"


                        android:onClick="smoke10"
                        android:layout_below="@+id/tsmoke1"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />

                    <Button
                        android:layout_width="fill_parent"
                        android:layout_height="75dp"
                        android:text="from outside a main to right side of red box"
                        android:id="@+id/tsmoke7"


                        android:onClick="smoke11"
                        android:layout_below="@+id/tsmoke6"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />
                    <Button
                        android:layout_width="fill_parent"
                        android:layout_height="75dp"
                        android:text="from outside squeaky to a catwalk"
                        android:id="@+id/tsmoke8"


                        android:onClick="smoke12"
                        android:layout_below="@+id/tsmoke7"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />
                    <Button
                        android:layout_width="fill_parent"
                        android:layout_height="75dp"
                        android:text="From outside squeaky to back of a site"
                        android:id="@+id/tsmoke9"


                        android:onClick="smoke13"
                        android:layout_below="@+id/tsmoke8"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />
                    <Button
                        android:layout_width="fill_parent"
                        android:layout_height="75dp"
                        android:text="From T white truck to a highway"
                        android:id="@+id/tsmoke10"


                        android:onClick="smoke14"
                        android:layout_below="@+id/tsmoke9"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />

</LinearLayout>
                    <android.support.v4.widget.Space
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"/>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:id="@+id/ctside"
                    android:layout_toRightOf="@id/tside"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="CT Side"
                        android:id="@+id/textView3"

                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_gravity="center_horizontal"/>
                <Button
                    android:layout_width="fill_parent"
                    android:layout_height="75dp"
                    android:text="from sandbags to boost"
                    android:id="@+id/ctsmoke5"

                    android:onClick="smoke2"
                    android:layout_above="@+id/tsmoke4"
                    android:layout_alignLeft="@+id/ctsmoke2"
                    android:layout_alignStart="@+id/ctsmoke2" />



                <Button
                    android:layout_width="fill_parent"
                    android:layout_height="75dp"
                    android:text="from a truck to a main"
                    android:id="@+id/ctsmoke4"

                    android:onClick="smoke4"

                    android:layout_below="@+id/ctsmoke5"
                    android:layout_alignLeft="@+id/ctsmoke5"
                    android:layout_alignStart="@+id/ctsmoke5" />



                <Button
                    android:layout_width="fill_parent"
                    android:layout_height="75dp"
                    android:text="from a truck to a door"
                    android:id="@+id/ctsmoke3"

                    android:onClick="smoke6"

                    android:layout_below="@+id/ctsmoke4"
                    android:layout_alignLeft="@+id/ctsmoke4"
                    android:layout_alignStart="@+id/ctsmoke4" />



                <Button
                    android:layout_width="fill_parent"
                    android:layout_height="75dp"
                    android:text="from b site to b main"
                    android:id="@+id/ctsmoke2"


                    android:onClick="smoke8"
                    android:layout_below="@+id/ctsmoke3"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true" />











                    </LinearLayout>
</LinearLayout>


            </RelativeLayout>
        </ScrollView>
    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        ads:adSize="BANNER"
        ads:adUnitId="ca-app-pub-6033901903111612/8199688884">
    </com.google.android.gms.ads.AdView>

</RelativeLayout>
